# ============================================
# Backend Environment Variables
# ============================================
# Copy this file to .env and fill in your values
# cp env.example .env

# ============================================
# üîê REQUIRED - Sui Blockchain
# ============================================
SUI_PRIVATE_KEY=your-sui-private-key-here
SUI_PACKAGE_ID=0x11aff42ade03a0c2e8e401b1a1c3b2e0c6b6c5f2d4e8f9a1b2c3d4e5f6a7b8c9
SEAL_POLICY_PACKAGE=0x11aff42ade03a0c2e8e401b1a1c3b2e0c6b6c5f2d4e8f9a1b2c3d4e5f6a7b8c9
SUI_NETWORK=testnet

# ============================================
# üåê Server Configuration
# ============================================
PORT=3001
FRONTEND_URL=http://localhost:3000

# ============================================
# üóÑÔ∏è Redis Configuration
# ============================================
REDIS_URL=redis://localhost:6379

# ============================================
# ü§ñ AI Services URLs
# ============================================
AI_DETECTION_URL=http://localhost:8000
REVERSE_SEARCH_URL=http://localhost:8002

# ============================================
# üîí Nautilus TEE (Optional - for production)
# ============================================
NAUTILUS_ENCLAVE_URL=
NAUTILUS_ENABLED=false
NAUTILUS_MRENCLAVE=
ENCLAVE_ID=mock_enclave_1

# ============================================
# ‚öôÔ∏è Multi-Worker Configuration (Optional)
# ============================================
USE_MULTI_WORKER=true
NUM_ENCLAVE_WORKERS=3
MIN_ENCLAVES=2
CONSENSUS_THRESHOLD=0.66

# ============================================
# üé≤ Oracle Configuration (Optional)
# ============================================
ORACLE_REPUTATION=0.8
ORACLE_STAKE=1000

# ============================================
# üì¶ Walrus Storage (Optional - has defaults)
